{
  "expo": {
    "name": "Razz",
    "slug": "pollapp",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/ios_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash_logo_resized.png",
      "backgroundColor": "#1e1e1e"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.jas1994.pollapp",
      "splash": {
        "image": "./assets/images/splash_logo_resized.png",
        "backgroundColor": "#1e1e1e"
      },
      "infoPlist":{
        "LSApplicationQueriesSchemes":["whatsapp"],
        "NSLocationWhenInUseUsageDescription": "This app requires access to your location to find your school.",
        "NSLocationAlwaysUsageDescription": "This app requires access to your location to find your school.",
        "NSLocationUsageDescription": "This app requires access to your location to find your school.",
        "NSCameraUsageDescription":"This app requires access to your camera to capture your profile picture.",
        "NSContactsUsageDescription":"This app requires access to your contacts to recommend and find your friends."
      },
      "buildNumber": "3"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app_logo.png",
        "backgroundColor": "#1e1e1e"
      },  
      "permissions": [],
      "package": "com.jas1994.pollapp",
      "versionCode": 10,
      "resolutions": {
        "mdpi": {
          "supportsTablet": false
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "34805afd-2276-43e6-b919-d485deafa99e"
      }
    },
    "plugins": [
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ],
      ["@logrocket/react-native"]
    ],
    "notification": {
      "icon": "./assets/images/razz_logo_96x96_greyscale.png"
    },
    "androidStatusBar": {
      "backgroundColor": "#1e1e1e",
      "translucent": false
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_ERROR_RECOVERY",
      "url": "https://u.expo.dev/34805afd-2276-43e6-b919-d485deafa99e"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
